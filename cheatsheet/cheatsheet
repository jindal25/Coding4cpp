audio hal code
device/renesas/dash5dv/hal/audio
hardware/libhardware/modules/usbaudio
=================================================================================================================================


62 # Audio
63 USE_XML_AUDIO_POLICY_CONF := 1
64 AUDIO_HAL_INTERFACE_VERSION := V4_0
65 
66 DEVICE_PACKAGE_OVERLAYS := $(LOCAL_PATH)/overlay
67 
68 PRODUCT_PACKAGES += \
69     audio.primary.dash5dv \
70     audio.usb.default \
71     android.hardware.automotive.audiocontrol@1.0-service.dash5dv
72 
73 ifeq ($(AUDIO_HAL_INTERFACE_VERSION),V4_0)
74     PRODUCT_PACKAGES += \
75         android.hardware.audio@4.0-service.dash5dv
76 else ifeq ($(AUDIO_HAL_INTERFACE_VERSION),V2_0)
77     PRODUCT_PACKAGES += \
78         android.hardware.audio@2.0-service.dash5dv
79 else
80     $(error "AUDIO_HAL_INTERFACE_VERSION isn't set to V2_0 nor V4_0")
81 endif
82 LOCAL_CFLAGS += -DAUDIO_HAL_VERSION=$(AUDIO_HAL_INTERFACE_VERSION)
83 
84 PRODUCT_COPY_FILES += \
85     device/renesas/dash5dv/hal/audio/audio_policy_configuration.xml:vendor/etc/audio_policy_configuration.xml \
86     device/renesas/dash5dv/hal/audio/audio_policy_volumes.xml:vendor/etc/audio_policy_volumes.xml \
87     device/renesas/dash5dv/hal/audio/default_volume_tables.xml:vendor/etc/default_volume_tables.xml \
88     device/renesas/dash5dv/hal/audio/usb_audio_policy_configuration.xml:vendor/etc/usb_audio_policy_configuration.xml

