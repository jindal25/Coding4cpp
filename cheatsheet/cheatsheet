brcm_patchram_plus --no2bytes --tosleep 50000 --baudrate 3000000 --use_baudrate_for_download --patchram /lib/firmware/brcm/CYW89359B1_002.002.014.0143.0295.hcd /dev/ttySC1
hciattach /dev/ttySC1 any 3000000 flow
hciconfig hci0 up
hcitool -i hci0 scan
mount / -o remount,rw 
